{
  "name": "bottle-recycler",
  "private": true,
  "version": "0.0.1",
  "type": "commonjs",
  "homepage": "https://rangerchris.github.io/BottleRecycler",
  "scripts": {
    "dev": "vite",
    "dev:coverage": "vite --mode coverage",
    "build": "tsc -b && vite build && tsc server.ts --outDir dist",
    "lint": "eslint .",
    "preview": "vite preview",
    "test": "npx playwright test --ui",
    "test:coverage": "npx playwright test",
    "run-tests-and-merge": "npx playwright test && node scripts/merge-coverage.js",
    "coverage": "start-server-and-test \"npm run dev:coverage\" http://localhost:5173 \"npm run run-tests-and-merge\"",
    "show-coverage": "npx playwright show-report coverage/report",
    "start": "node dist/server.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  },
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.1",
    "@mui/material": "^7.3.1",
    "express": "^5.1.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "express-rate-limit": "^8.0.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@playwright/test": "^1.55.0",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.11",
    "@types/react-dom": "^19.1.8",
    "@vitejs/plugin-react": "^5.0.1",
    "esbuild": "^0.25.9",
    "eslint": "^9.34.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "gh-pages": "^6.3.0",
    "globals": "^16.3.0",
    "ts-node": "^10.9.2",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.41.0",
    "vite": "^7.1.3",
    "vite-plugin-istanbul": "^3.0.0",
    "istanbul-lib-coverage": "^3.0.0",
    "istanbul-lib-report": "^3.0.0",
    "istanbul-reports": "^3.0.0",
    "start-server-and-test": "^1.14.0"
  }
}
